datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model User {
    id              Int         @id @default(autoincrement())
    username        String      @unique
    email           String      @unique
    password        String
    name            String
    cpf             String
    birth           String
    phone           String      
    pronoun         String
    uf              String
    admin           Boolean     @default(false)

    image           String?     @db.Text

    google_id       String?
    google_token    String?     @db.Text
}
